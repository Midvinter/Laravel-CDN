{
    "name": "engagement-agency/cdn",
    "description": "A CDN helper for laravel",
    "keywords": ["laravel", "cdn"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joel E. Svensson",
            "email": "joel.e.svensson@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "symfony/filesystem": "^2.7"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
	    "mikey179/vfsStream": "~1.6"
    },
    "autoload": {
        "psr-4": {
            "EngagementAgency\\CDN\\": "src/"
        },
        "autoload-dev": {
            "psr-4": {
                "spec\\EngagementAgency\\CDN\\": "spec/",
                "tests\\EngagementAgency\\CDN\\": "tests/"
            }
        }
    }
}
